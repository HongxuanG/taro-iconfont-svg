{
  "name": "taro-iconfont-svg",
  "version": "1.1.5",
  "description": "让Taro也能使用阿里矢量图标库的SVG",
  "keywords": [
    "taro",
    "iconfont",
    "svg"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HongxuanG/taro-iconfont-svg.git"
  },
  "author": "ghx",
  "main": "index.js",
  "bin": {
    "iconfont-init": "./commands/createJson.js",
    "iconfont-taro": "./commands/index.js"
  },
  "scripts": {
    "build": "pnpm run --filter core build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp package.json --commit --push --tag",
    "iconfont-init": "npx ts-node packages/core/src/commands/createJson.ts --config packages/examples/iconfont.json",
    "iconfont-taro": "npx ts-node packages/core/src/commands/index.ts --config packages/examples/iconfont.json"
  },
  "dependencies": {
    "@tarojs/taro": "3.6.0",
    "classnames": "^2.3.2",
    "colors": "^1.4.0",
    "fs-extra": "^11.1.0",
    "glob": "^8.0.3",
    "iconfont-parser": "^1.0.0",
    "minimist": "^1.2.7",
    "mkdirp": "^1.0.4",
    "svgo": "^3.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@aaronghx/eslint-config-ts": "^0.1.11",
    "@tarojs/components": "3.6.0",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^8.0.0",
    "@types/minimist": "^1.2.2",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.11.15",
    "@types/xml2js": "0.4.5",
    "bumpp": "^8.2.1",
    "eslint": "^8.29.0",
    "react": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "homepage": "https://github.com/HongxuanG/taro-iconfont-svg",
  "packageManager": "pnpm@9.12.0+sha512.4abf725084d7bcbafbd728bfc7bee61f2f791f977fd87542b3579dcb23504d170d46337945e4c66485cd12d588a0c0e570ed9c477e7ccdd8507cf05f3f92eaca"
}
